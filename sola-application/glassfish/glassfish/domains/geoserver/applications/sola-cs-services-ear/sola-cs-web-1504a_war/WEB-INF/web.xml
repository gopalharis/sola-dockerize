    <context-param>
        <param-name>javax.faces.PROJECT_STAGE</param-name>
        <param-value>Development</param-value>
    </context-param>
    <context-param>
        <param-name>javax.faces.INTERPRET_EMPTY_STRING_SUBMITTED_VALUES_AS_NULL</param-name>
        <param-value>true</param-value>
    </context-param>
    <servlet>
        <servlet-name>captcha</servlet-name>
        <servlet-class>org.sola.opentenure.services.boundary.beans.helpers.CaptchaImage</servlet-class>
        <init-param>
            <description>passing height</description>
            <param-name>height</param-name>
            <param-value>40</param-value>
        </init-param>
        <init-param>
            <description>passing height</description>
            <param-name>width</param-name>
            <param-value>120</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Faces Servlet</servlet-name>
        <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>GetAttachment</servlet-name>
        <servlet-class>org.sola.opentenure.services.boundary.beans.claim.GetAttachment</servlet-class>
    </servlet>
