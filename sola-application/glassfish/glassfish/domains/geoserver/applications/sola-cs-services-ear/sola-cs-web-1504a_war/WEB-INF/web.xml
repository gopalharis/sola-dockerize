            <description/>
            <url-pattern>/Dashboard.xhtml</url-pattern>
            <url-pattern>/user/profile.xhtml</url-pattern>
            <url-pattern>/claim/Claim.xhtml</url-pattern>
            <url-pattern>/claim/ClaimPrint.xhtml</url-pattern>
